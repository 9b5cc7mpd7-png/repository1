# Application mode
VITE_APP_ENV=development

# Backend API (use full origin, e.g. https://api.example.com)
VITE_API_BASE_URL=http://localhost:5173

# Mock API in development (true/false)
VITE_ENABLE_API_MOCKS=true

# Optional release identifier for monitoring
VITE_RELEASE=local-dev

# Optional Sentry settings
VITE_SENTRY_DSN=
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1

# Optional endpoint for web-vitals reporting
VITE_WEB_VITALS_ENDPOINT=/api/web-vitals

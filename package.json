{
  "name": "visual-dashboard",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --maxWorkers=1",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --maxWorkers=1",
    "check:repo-hygiene": "bash scripts/check-repo-hygiene.sh",
    "check:security": "npm audit --audit-level=moderate",
    "privacy:status": "bash scripts/privacy-check.sh",
    "privacy:enforce": "bash scripts/privacy-check.sh --strict",
    "check:all": "npm run check:repo-hygiene && npm run typecheck && npm run test && npm run build && npm run check:security"
  },
  "dependencies": {
    "@sentry/react": "^8.55.0",
    "@tanstack/react-query": "^5.90.20",
    "clsx": "^2.1.1",
    "msw": "^2.12.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "recharts": "^2.15.4",
    "web-vitals": "^4.2.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.10.11",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.1.3",
    "jsdom": "^27.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
